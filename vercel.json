{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend",
  "framework": null,
  "functions": {
    "api/index.js": {
      "includeFiles": "cache/**"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" }
  ],
  "env": {
    "GTFS_RT_VEHICLES_URL": "https://www.myridebarrie.ca/gtfs/GTFS_VehiclePositions.pb",
    "POLL_MS": "10000"
  },
  "build": {
    "env": {
      "GTFS_STATIC_URL": "https://www.myridebarrie.ca/gtfs/google_transit.zip"
    }
  }
}
